//program 1
import java.util.*;

//average of 3 numbers
public class solution {
    public static void main(String args[]) {
        Scanner sc  = new Scanner(System.in);
        
        int A = sc.nextInt();
        int B = sc.nextInt();
        int C = sc.nextInt();   
        
        int average = (A + B + C) / 3;

        System.out.println("average is : " + average);
    }
}

//program 2
import java.util.*;

public class area {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int side = sc.nextInt();

        int area = side  * side;

        System.out.println("area of the square is : " + area);

    }

}
